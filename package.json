{
  "name": "glud-component",
  "version": "1.10.1",
  "description": "",
  "repository": {
    "url": "",
    "type": "git"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "build",
    "configs"
  ],
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "better-npm-run dev",
    "deploy": "cp now.json styleguide/ && now ./styleguide && now alias",
    "build:lib": "better-npm-run clean:lib && babel src/components --out-dir lib",
    "build": "npm run clean && better-npm-run build",
    "build:client": "npm run clean && better-npm-run build:client",
    "start": "better-npm-run start",
    "clean": "better-npm-run clean",
    "test": "npm run lint && better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "lint": "eslint ./src",
    "prepublish": "npm run build:lib && npm run build && npm run styleguide:build",
    "postpublish": "npm run deploy",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "betterScripts": {
    "dev": {
      "command": "node ./scripts/devServer.js --progress --hide-modules --config ./configs/webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development",
        "BUILD_ENV": "server"
      }
    },
    "build": {
      "command": "webpack --progress --hide-modules --config ./configs/webpack.config.production.js",
      "env": {
        "NODE_ENV": "production",
        "BUILD_ENV": "server"
      }
    },
    "build:client": {
      "command": "webpack --progress --hide-modules --config ./configs/webpack.config.production.js",
      "env": {
        "NODE_ENV": "production",
        "BUILD_ENV": "client"
      }
    },
    "start": {
      "command": "node ./scripts/start.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha ./test/*.js --compilers js:babel-register",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test:watch": {
      "command": "mocha ./test/*.js --compilers js:babel-register --watch",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "clean": {
      "command": "rimraf build"
    },
    "clean:lib": {
      "command": "rimraf lib"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.12",
    "bulma-tooltip": "^2.0.2",
    "classnames": "^2.2.5",
    "cleave.js": "^1.4.7",
    "moment": "^2.24.0",
    "moment-range": "^3.1.1",
    "react-daterange-picker": "^2.0.1",
    "react-powerplug": "^0.1.4",
    "react-select": "^2.1.2",
    "react-spring": "^8.0.24"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.2.0",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "8.2.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "better-npm-run": "0.0.15",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.28.1",
    "es6-promise": "^4.0.5",
    "eslint": "^4.7.2",
    "eslint-config-standard": "^10.2.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "expect": "^1.20.2",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.0",
    "promise-polyfill": "^7.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dev-utils": "^0.5.2",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.0.3",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-styleguidist": "^7.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.17.0",
    "webpack-isomorphic-tools": "^3.0.5"
  },
  "peerDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
